@model Connected.Service.ArticleService.Article[]

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Page Title</title>
</head>
<body>

    <h1>Articles</h1>

    <table>
        @foreach (var article in Model)
        {
            <tr>
                <td>@article.Title</td>
                <td><a href="@article.URL" target="_blank">Link</a></td>
                <td>@article.Author</td>
                <td>@article.Contents</td>
            </tr>
        }
    </table>



</body>
</html>